{
  "dashboard": {
    "id": null,
    "title": "PromptPilot Metrics Dashboard",
    "tags": ["promptpilot", "llm", "prometheus"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "Prompt Executions Over Time",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(prompt_executions_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Executions/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Total Prompt Executions",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(prompt_executions_total)",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Active Users",
        "gridPos": {
          "x": 18,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "active_users",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 4,
        "type": "graph",
        "title": "Average Response Time",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "avg(prompt_response_time_seconds)",
            "legendFormat": "Average Response Time",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Response Time (seconds)"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "type": "graph",
        "title": "Tokens Used Over Time",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(tokens_used_total[5m])",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Tokens/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "type": "piechart",
        "title": "Prompt Executions by Provider",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (provider) (prompt_executions_total)",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "type": "piechart",
        "title": "Prompt Executions by Model",
        "gridPos": {
          "x": 8,
          "y": 16,
          "w": 8,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (model) (prompt_executions_total)",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "type": "stat",
        "title": "Total Cost",
        "gridPos": {
          "x": 16,
          "y": 16,
          "w": 8,
          "h": 4
        },
        "targets": [
          {
            "expr": "total_cost_dollars",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "prefix": "$"
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}