{
  "dashboard": {
    "id": null,
    "title": "PromptPilot Monitoring",
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "type": "graph",
        "title": "HTTP Requests Rate",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} {{status}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "type": "graph",
        "title": "HTTP Request Duration",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile",
            "refId": "B"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Total Prompts",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "prompt_total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Total Pipelines",
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "pipeline_total",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          }
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Active Users",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "active_users",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          }
        }
      },
      {
        "id": 6,
        "type": "stat",
        "title": "System Uptime",
        "gridPos": {
          "x": 18,
          "y": 8,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "process_uptime_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": [
              "lastNotNull"
            ]
          },
          "orientation": "horizontal",
          "textMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 7,
        "type": "graph",
        "title": "Prompt Executions",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(prompt_executions_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Executions/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 8,
        "type": "graph",
        "title": "Pipeline Executions",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(pipeline_executions_total[5m])",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Executions/sec"
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ],
      "time_options": [
        "5m",
        "15m",
        "1h",
        "6h",
        "12h",
        "24h",
        "2d",
        "7d",
        "30d"
      ]
    }
  }
}