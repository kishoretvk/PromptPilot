{
  "name": "promptpilot-dashboard",
  "version": "1.0.0",
  "description": "PromptPilot Dashboard - The Ultimate Prompt Engineering and LLM Workflow Platform",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "craco start",
    "start:prod": "cross-env REACT_APP_ENV=production craco start",
    "build": "craco build",
    "build:dev": "cross-env REACT_APP_ENV=development craco build",
    "build:analyze": "ANALYZE=true craco build",
    "test": "craco test",
    "test:watch": "jest --watch",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "craco test --coverage --ci --silent --detectOpenHandles --watchAll=false",
    "test:debug": "jest --debug",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:unit": "jest --testPathPattern=/__tests__/ --testNamePattern='unit'",
    "test:integration": "jest --testPathPattern=/integration/ --testNamePattern='integration'",
    "test:components": "jest --testPathPattern=/components/",
    "test:services": "jest --testPathPattern=/services/",
    "test:hooks": "jest --testPathPattern=/hooks/",
    "test:utils": "jest --testPathPattern=/utils/",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "eject": "craco eject",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "analyze": "craco build && npx source-map-explorer 'build/static/js/*.js'",
    "serve": "serve -s build -l 3000",
    "serve:prod": "serve -s build -l 3000 --single",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "precommit": "lint-staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docker:build": "docker build -t promptpilot-frontend .",
    "docker:run": "docker run -p 3000:80 promptpilot-frontend",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test:ci"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "^5.0.0-alpha.152",
    "@mui/material": "^5.14.17",
    "@mui/x-data-grid": "^6.18.1",
    "@tanstack/react-query": "^5.8.5",
    "@tanstack/react-query-devtools": "^5.8.5",
    "axios": "^1.6.0",
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-diff-viewer": "^3.1.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.47.0",
    "react-router-dom": "^6.18.0",
    "react-toastify": "^11.0.5",
    "reactflow": "^11.10.1",
    "recharts": "^2.9.0",
    "uuid": "^9.0.1",
    "web-vitals": "^5.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@craco/craco": "^5.9.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@storybook/test": "^7.5.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.0.3",
    "react-scripts": "^5.0.1",
    "serve": "^10.0.2",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.2.2",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "prompt-engineering",
    "llm",
    "ai",
    "workflow",
    "react",
    "fastapi",
    "typescript"
  ],
  "author": "PromptPilot Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/promptpilot.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/promptpilot/issues"
  },
  "homepage": "."
}
