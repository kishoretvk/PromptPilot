  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^9.0.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^27.5.1",
    "jest-transform-css": "^6.0.1",
    "jest-transform-file": "^1.1.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.10",
    "msw": "^2.6.4",
    "prettier": "^3.4.2",
    "serve": "^14.2.1",
    "source-map-explorer": "^2.5.3",
    "storybook": "^8.3.6",
    "ts-jest": "^27.1.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:prod": "cross-env REACT_APP_ENV=production react-scripts start",
    "build": "react-scripts build",
    "build:dev": "cross-env REACT_APP_ENV=development react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --silent --detectOpenHandles --watchAll=false",
    "test:debug": "jest --debug",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:unit": "jest --testPathPattern=/__tests__/ --testNamePattern='unit'",
    "test:integration": "jest --testPathPattern=/integration/ --testNamePattern='integration'",
    "test:components": "jest --testPathPattern=/components/",
    "test:services": "jest --testPathPattern=/services/",
    "test:hooks": "jest --testPathPattern=/hooks/",
    "test:utils": "jest --testPathPattern=/utils/",
    "test:ci": "react-scripts test --coverage --ci --silent --detectOpenHandles --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "serve": "serve -s build -l 3000",
    "serve:prod": "serve -s build -l 3000 --single",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docker:build": "docker build -t promptpilot-frontend .",
    "docker:run": "docker run -p 3000:80 promptpilot-frontend",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test:ci"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "proxy": "http://localhost:8000",
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:ci"
    }
  }
}
